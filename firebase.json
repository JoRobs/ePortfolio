{
  "functions": {
    "predeploy": [
      "npm --prefix ./functions run lint"
    ],
    "source": "functions"
  },
  "hosting": {
    "predeploy":[
      "npm --prefix ./Front-End/everest run build"
    ],
    "public": "Front-End/everest/build",
    "ignore": [
      "firebase.json",
      "**/node_modules/**"
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    },
    "rewrites": [{
      "source": "**",
      "function": "app"
    }]
  }
}
